{
  "name": "typescript-practice-shop-manager",
  "version": "1.0.0",
  "description": "learn typescript from nhatduong",
  "main": "src/ts/main/*.ts",
  "source": [
    "src/pages/login.html",
    "src/pages/register.html",
    "src/pages/dashboard.html",
    "src/pages/homepage.html"
  ],
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "heroku-postbuild": "yarn build && yarn start",
    "start": "concurrently --kill-others \"npx parcel src/ts/main/*.ts src/assets/** src/pages/*.html\" \"node server.js\" \"parcel src/pages/dashboard.html\"",
    "api": "json-server db.json",
    "dev": "concurrently --kill-others \"npx run build\" \"npx run start\" \"npx run api\"",
    "lint": "eslint",
    "check": "tsc --noEmit",
    "ci": "yarn build && yarn test && yarn lint && yarn check"
  },
  "keywords": [
    "typescript"
  ],
  "repository": "https://gitlab.com/walkchicken/typescript-practice-shop-manager.git",
  "author": "Nhatduong <dcnhat1862000@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@parcel/transformer-sass": "2.6.0",
    "@types/jest": "^28.1.1",
    "@types/json-server": "^0.14.4",
    "concurrently": "^7.2.2",
    "jest": "^28.1.1",
    "json-server": "^0.17.0",
    "npx": "^10.2.2",
    "parcel": "^2.6.2",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-asset-copier": "^1.1.1",
    "serve": "^13.0.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.6.0",
    "@types/jest": "^28.1.1",
    "@types/json-server": "^0.14.4",
    "concurrently": "^7.2.2",
    "jest": "^28.1.1",
    "json-server": "^0.17.0",
    "parcel": "^2.6.2",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-asset-copier": "^1.1.1",
    "serve": "^13.0.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.3"
  },
  "assetsPath": "src/assets"
}
